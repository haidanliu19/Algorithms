        "def digit_reverse(n):\n",
        "    a=[]\n",
        "\n",
        "    while n>1:\n",
        "        a.append(int(n%10))\n",
        "        n/=10\n",
        "\n",
        "\n",
        "    return a\n",
        "digit_reverse(n)"
      ],
