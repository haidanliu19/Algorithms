      "source": [
        "def solution(numbers):\n",
        "    answer = list(set([(numbers[i]+numbers[j]) for i in range(len(numbers)) for j in range(i+1, len(numbers))]))\n",
        "    return sorted(answer)\n",
        "\n",
        "numbers = [2,1,3,4,1]\n",
        "print(solution(numbers))\n",
        "numbers = [5,0,2,7]\n",
        "print(solution(numbers))"
