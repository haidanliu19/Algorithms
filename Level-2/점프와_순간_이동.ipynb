      "source": [
        "def solution(n):\n",
        "    ans = 0\n",
        "    while n > 0:\n",
        "        if n % 2 != 0:\n",
        "            n = n -1 \n",
        "            ans += 1\n",
        "        else:\n",
        "            n  = n // 2\n",
        "    return ans\n",
        "\n",
        "n = 5000\n",
        "solution(n)"
      ]
