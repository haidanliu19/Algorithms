      "source": [
        "def perm(input, i):\n",
        "    if i == len(input) - 1:\n",
        "        print(input)\n",
        "    else:\n",
        "        for j in range(i, len(input)):\n",
        "            input[i], input[j] = input[j], input[i]\n",
        "            perm(input, i+1)\n",
        "            input[i], input[j] = input[j], input[i]\n",
        "perm(['A', 'B', 'C'], 0)"
