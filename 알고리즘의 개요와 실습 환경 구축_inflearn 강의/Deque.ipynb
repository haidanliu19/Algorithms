      "source": [
        "from collections import deque\n",
        "\n",
        "deq = deque([7])\n",
        "print(\"7 입력 :\",deq)\n",
        "\n",
        "deq.append(5)\n",
        "print(\"5 입력 :\",deq) # 오른쪽으로 넣기\n",
        "\n",
        "deq.appendleft(4)\n",
        "print(\"4 입력 :\",deq) # 왼쪽으로 넣기\n",
        "\n",
        "deq.popleft()\n",
        "print(\"완쪽 삭제 :\",deq)\n",
        "\n",
        "deq.pop()\n",
        "print(\"오른쪽 삭제 :\",deq)"
      ],
      "execution_count": null,
